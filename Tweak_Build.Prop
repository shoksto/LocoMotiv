#Disabilitare le  ADB  notification Icon
persist.adb.notify=0

#Abilitare l'Hardware Rendering of the GUI
debug.sf.hw=1
video.accelerate.hw=1            <---- alcuni lamentano problemi, personalmente sempre usato senza problemi

#Miglioria scrolling
windowsmgr.max_events_per_sec=160

#Disabilitare Google OTA Update checkin
ro.config.nocheckin=1

#Disabilitare Google Report service
profiler.force_disable_err_rpt=1
profiler.force_disable_ulog=1
